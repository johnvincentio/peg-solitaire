{
    "sass.enabled": false,
    "path": {
        "**/scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "includePaths": [
                    "**/scss/"
                ],
                "imagePath": null,
                "sourceComments": true,
                "outputStyle": "expanded"
            },
            "linting.collapsed": true
        }
    }
}